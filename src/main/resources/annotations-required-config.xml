<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <!--вкл конфигурирование по аннотациям(поумолчанию откл)-->
    <context:annotation-config/>

    <bean id="developer" class="educating_spring.annotations.required.Developer">
        <property name="name" value="BOBA"/>
        <property name="experience" value="3"/>
        <property name="specialty" value=" Android Developer"/>

    </bean>
</beans>
